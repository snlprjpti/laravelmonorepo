{
    "name": "symplify\/symplify-kernel",
    "description": "Internal Kernel for Symplify packages",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony\/console": "^5.4|^6.0",
        "symfony\/dependency-injection": "^5.4|^6.0",
        "symplify\/smart-file-system": "dev-main",
        "symplify\/composer-json-manipulator": "dev-main",
        "symplify\/autowire-array-parameter": "dev-main",
        "symplify\/package-builder": "dev-main",
        "symplify\/symplify-kernel": "dev-main",
        "webmozart\/assert": "^1.10"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MonorepoBuilder20211223\\Symplify\\SymplifyKernel\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.1-dev"
        }
    },
    "conflict": {
        "symplify\/astral": "<10.0.6",
        "symplify\/easy-coding-standard": "<10.0.6",
        "symplify\/phpstan-rules": "<10.0.6",
        "symplify\/easy-testing": "<10.0.6",
        "symplify\/rule-doc-generator-contracts": "<10.0.6",
        "symplify\/symfony-php-config": "<10.0.6",
        "symplify\/simple-php-doc-parser": "<10.0.6",
        "symplify\/php-config-printer": "<10.0.6",
        "symplify\/console-color-diff": "<10.0.6",
        "symplify\/markdown-diff": "<10.0.6",
        "symplify\/amnesia": "<10.0.6",
        "symplify\/phpstan-extensions": "<10.0.6",
        "symplify\/rule-doc-generator": "<10.0.6",
        "symplify\/vendor-patches": "<10.0.6",
        "symplify\/skipper": "<10.0.6",
        "symplify\/symfony-static-dumper": "<10.0.6",
        "symplify\/git-wrapper": "<10.0.6",
        "symplify\/monorepo-builder": "<10.0.6",
        "symplify\/config-transformer": "<10.0.6",
        "symplify\/easy-ci": "<10.0.6",
        "symplify\/coding-standard": "<10.0.6",
        "symplify\/latte-phpstan-compiler": "<10.0.6",
        "symplify\/template-phpstan-compiler": "<10.0.6",
        "symplify\/phpstan-latte-rules": "<10.0.6",
        "symplify\/easy-parallel": "<10.0.6",
        "symplify\/neon-config-dumper": "<10.0.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}