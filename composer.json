{
    "name": "sunil/laravelmonorepo",
    "authors": [
        {
            "name": "Sunil Prajapati",
            "email": "sunil.prajapati@hazesoft.co"
        }
    ],
    "require": {
        "php": "^8.0.8",
        "symplify/monorepo-builder": "^10.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sunil\\Laravelmonorepo\\": "src/",
            "YourMonorepo\\FirstPackage\\": "Modules/attribute/"
        }
    },
    "replace": {
        "snlprjti/first-package": "self.version"
    }
}
